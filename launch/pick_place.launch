<launch>
    <arg name="method" default="3"/>

    <arg name="scene" default="pick_place2"/>
    <arg name="object" default="object2"/>
    <arg name="pick_pose" default="pick2"/>
    <arg name="place_pose" default="place"/>
    <arg name="arm_speed" default="0.2"/>
    <arg name="gripper_speed" default="0.1"/>
    <arg name="grasp_width" default="0.02"/>
    <arg name="grasp_force" default="10"/>
    <arg name="grasp_epsilon_inner" default="0.02"/>
    <arg name="grasp_epsilon_outer" default="0.02"/>

    <arg name="pre_grasp_approch_x" default="0.00"/>
    <arg name="pre_grasp_approch_y" default="0.00"/>
    <arg name="pre_grasp_approch_z" default="0.10"/>

    <arg name="post_grasp_retreat_x" default="0.00"/>
    <arg name="post_grasp_retreat_y" default="0.00"/>
    <arg name="post_grasp_retreat_z" default="0.10"/>

    <arg name="post_place_retreat_x" default="-0.10"/>
    <arg name="post_place_retreat_y" default="0.00"/>
    <arg name="post_place_retreat_z" default="0.00"/>

    <node name="pick_place" pkg="panda_controller" 
    type="pick_place" respawn="false" output="screen">
        <param name="method" value="$(arg method)"/>
        <param name="scene" value="$(arg scene)"/>
        <param name="object" value="$(arg object)"/>
        <param name="pick_pose" value="$(arg pick_pose)"/>
        <param name="place_pose" value="$(arg place_pose)"/>
        <param name="arm_speed" value="$(arg arm_speed)"/>
        <param name="gripper_speed" value="$(arg gripper_speed)"/>
        <param name="grasp_width" value="$(arg grasp_width)"/>
        <param name="grasp_force" value="$(arg grasp_force)"/>
        <param name="grasp_epsilon_inner" value="$(arg grasp_epsilon_inner)"/>
        <param name="grasp_epsilon_outer" value="$(arg grasp_epsilon_outer)"/>

        <param name="pre_grasp_approch_x" value="$(arg pre_grasp_approch_x)"/>
        <param name="pre_grasp_approch_y" value="$(arg pre_grasp_approch_y)"/>
        <param name="pre_grasp_approch_z" value="$(arg pre_grasp_approch_z)"/>
        <param name="post_grasp_retreat_x" value="$(arg post_grasp_retreat_x)"/>
        <param name="post_grasp_retreat_y" value="$(arg post_grasp_retreat_y)"/>
        <param name="post_grasp_retreat_z" value="$(arg post_grasp_retreat_z)"/>
        <param name="post_place_retreat_x" value="$(arg post_place_retreat_x)"/>
        <param name="post_place_retreat_y" value="$(arg post_place_retreat_y)"/>
        <param name="post_place_retreat_z" value="$(arg post_place_retreat_z)"/>
    </node>
</launch>
